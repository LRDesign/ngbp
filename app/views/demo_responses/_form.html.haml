= form_for @demo_response do |f|
  - if @demo_response.errors.any?
    #error_explanation
      %h2= "#{pluralize(@demo_response.errors.count, "error")} prohibited this demo_response from being saved:"
      %ul
        - @demo_response.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :demo_answer_id
    = f.number_field :demo_answer_id
  .field
    = f.label :created_at
    = f.datetime_select :created_at
  .field
    = f.label :updated_at
    = f.datetime_select :updated_at
  .actions
    = f.submit 'Save'
