= form_for @study_question do |f|
  - if @study_question.errors.any?
    #error_explanation
      %h2= "#{pluralize(@study_question.errors.count, "error")} prohibited this study_question from being saved:"
      %ul
        - @study_question.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :text
    = f.text_area :text
  .field
    = f.label :study_id
    = f.number_field :study_id
  .field
    = f.label :attachment_file_name
    = f.text_field :attachment_file_name
  .field
    = f.label :attachment_content_type
    = f.text_field :attachment_content_type
  .field
    = f.label :attachment_file_size
    = f.number_field :attachment_file_size
  .field
    = f.label :attachment_updated_at
    = f.datetime_select :attachment_updated_at
  .field
    = f.label :embed
    = f.text_area :embed
  .field
    = f.label :created_at
    = f.datetime_select :created_at
  .field
    = f.label :updated_at
    = f.datetime_select :updated_at
  .actions
    = f.submit 'Save'
