    = f.label :email
    = f.text_field :email
  .field
    = f.label :password
    = f.text_field :password
  .field
    = f.label :paypal_email
    = f.text_field :paypal_email
  .field
    = f.label :gender
    = f.number_field :gender
  .field
    = f.label :yob
    = f.number_field :yob
  .field
    = f.label :country
    = f.text_field :country
  .field
    = f.label :zip
    = f.text_field :zip
  .field
    = f.label :time_zone
    = f.text_field :time_zone
  .field
    = f.label :type
    = f.text_field :type
  .field
    = f.label :rating_cache
    = f.text_field :rating_cache
  .field
    = f.label :company
    = f.text_field :company
  .field
    = f.label :city
    = f.text_field :city
